pp=http://www.hshahrokhi.ir/
mkdir fonts
mkdir fonts/eot
mkdir fonts/woff2
mkdir fonts/woff
mkdir ttf

cd fonts/eot
wget $pp/fonts/eot/IRANSansWeb_Bold.eot
wget $pp/fonts/eot/IRANSansWeb_Bold.eot
wget $pp/fonts/eot/IRANSansWeb_Medium.eot
wget $pp/fonts/eot/IRANSansWeb_Medium.eot
wget $pp/fonts/eot/IRANSansWeb_Light.eot
wget $pp/fonts/eot/IRANSansWeb_Light.eot
wget $pp/fonts/eot/IRANSansWeb_UltraLight.eot
wget $pp/fonts/eot/IRANSansWeb_UltraLight.eot
wget $pp/fonts/eot/IRANSansWeb.eot
wget $pp/fonts/eot/IRANSansWeb.eot

cd ../woff2
wget $pp/fonts/woff2/IRANSansWeb_Bold.woff2
wget $pp/fonts/woff2/IRANSansWeb.woff2
wget $pp/fonts/woff2/IRANSansWeb_Light.woff2
wget $pp/fonts/woff2/IRANSansWeb_Medium.woff2
wget $pp/fonts/woff2/IRANSansWeb_UltraLight.woff2

cd ../woff
wget $pp/fonts/woff/IRANSansWeb_Medium.woff
wget $pp/fonts/woff/IRANSansWeb_Bold.woff
wget $pp/fonts/woff/IRANSansWeb_UltraLight.woff
wget $pp/fonts/woff/IRANSansWeb.woff
wget $pp/fonts/woff/IRANSansWeb_Light.woff

cd ../ttf
wget $pp/fonts/ttf/IRANSansWeb_Medium.ttf
wget $pp/fonts/ttf/IRANSansWeb_Light.ttf
wget $pp/fonts/ttf/IRANSansWeb_UltraLight.ttf
wget $pp/fonts/ttf/IRANSansWeb_Bold.ttf
wget $pp/fonts/ttf/IRANSansWeb.ttf

cd ..
wget $pp/fonts/fontawesome-webfont.eot?v=4.5.0
wget $pp/fonts/fontawesome-webfont.eot?#iefix&v=4.5.0
wget $pp/fonts/fontawesome-webfont.woff2?v=4.5.0
wget $pp/fonts/fontawesome-webfont.woff?v=4.5.0
wget $pp/fonts/fontawesome-webfont.ttf?v=4.5.0
wget $pp/fonts/fontawesome-webfont.svg?v=4.5.0#fontawesomeregular